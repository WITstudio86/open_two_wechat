{
  "productName": "more_wechat",
  "version": "0.2.4",
  "identifier": "WITstudio",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "trunk serve",
    "beforeBuildCommand": "trunk build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "WITStudio",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
